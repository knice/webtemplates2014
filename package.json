{
    "name": "ucsc-responsive-templates",
    "description": "Responsive reboot of the 2010 UCSC web templates.",
    "repository": "https://github.com/ucsc/webtemplates2014/",
    "version": "2.0.0",
    "main": "index.js",
    "scripts": {
        "dev": "gulp clean && parcel index.html",
        "build": "gulp clean && parcel build index.html --out-dir build"
    },
    "author": "Rob Knight",
    "license": "BSD-2-Clause",
    "dependencies": {
        "fancybox": "^3.0.1",
        "gulp": "^4.0.2",
        "jquery": "^3.4.1",
        "modernizr": "^3.8.0",
        "owlcarousel": "^1.3.3",
        "utilities": "^1.0.5"
    },
    "frontend": {
        "name": "Web templates for UC Santa Cruz",
        "stylesheets": "src/sass",
        "javascripts": "src/js"
    },
    "devDependencies": {
        "autoprefixer": "^9.7.4",
        "cheerio": "^0.22.0",
        "del": "^1.1.1",
        "frontend-md": "0.0.4",
        "mkdirp": "^0.5.1",
        "parcel-bundler": "^1.12.4",
        "parcel-plugin-clean-dist": "0.0.6",
        "parcel-plugin-static-files-copy": "^2.2.2",
        "parcel-plugin-structurize": "^1.1.6",
        "sass": "^1.25.0"
    },
    "parcel-plugin-structurize": {
        "scripts": {
            "match": "*.{js,js.map}",
            "folder": "js"
        },
        "styles": {
            "match": "*.{css,css.map}",
            "folder": "css"
        },
        "assets": {
            "match": "*.{png,svg,jpg,jpg2,jpeg,gif,bmp,webm}",
            "folder": "images"
        }
    },
    "staticFiles": {
        "staticPath": "src/archive",
        "watcherGlob": "**"
    },
    "browserslist": [
        "defaults",
        "not IE 11",
        "not IE_Mob 11",
        "maintained node versions"
    ]
}
